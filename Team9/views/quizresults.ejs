<%- include('bootheader') -%>
<%- include('menubar', {variant: 'quiz'}) -%>

<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <div class="jumbotron">
      <h2><strong>According to your quiz results, you might be interested in:</strong></h2>
      <ul class="list-group list-group-flush">
        <% nearest.map((near) => { %>
          <li class="list-group-item"><h3><a href="<%= near.address %>"><%= near.app_name %></a></h3> by <a href="/bios/<%=near.name.split(" ")[0].toLowerCase()%>"><%= near.name %></a></li>
        <% }) %>
      </ul>
    </div>
    <div>
      <h5>Want to <a href="/quiz">try again?</a></h5>
    </div>
  </body>
</html>
